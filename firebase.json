{
  "hosting": {
    "public": "dist/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/ws",
        "function": "websocket"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}